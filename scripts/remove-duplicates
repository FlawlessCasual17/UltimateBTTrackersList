#!/bin/bash

# Read the content of the file into an array
content=($(cat ultimate_trackers.txt))

# Create an associative array to count occurrences
declare -A counts=()

# Count occurrences of each line
for line in "${content[@]}"; do
  ((counts[$line]++))
done

# Find and print duplicate lines
for line in "${!counts[@]}"; do
  if [[ ${counts[$line]} -gt 1 ]]; then
    echo "Duplicate found: $line"
  fi
done

# Write unique lines to the file
printf "%s\n" "${content[@]}" | sort -u > ultimate_trackers.txt
